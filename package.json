{
  "name": "@croupier/sdk",
  "version": "1.0.0",
  "description": "Croupier TypeScript/JavaScript SDK with NNG transport",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "example": "node dist/examples/basic.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "croupier",
    "sdk",
    "nng",
    "game-server",
    "rpc",
    "typescript"
  ],
  "author": "cuihairu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cuihairu/croupier.git",
    "directory": "sdks/ts"
  },
  "dependencies": {
    "nng.js": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
