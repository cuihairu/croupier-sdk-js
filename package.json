{
  "name": "croupier-sdk",
  "version": "1.0.0",
  "description": "Croupier Node.js SDK for Game Function Registration with Hot Reload Support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "croupier",
    "game",
    "sdk",
    "grpc",
    "hotreload",
    "nodejs",
    "typescript",
    "gaming",
    "backend"
  ],
  "author": "Croupier Team <dev@croupier.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuihairu/croupier-sdk-js.git"
  },
  "homepage": "https://github.com/cuihairu/croupier-sdk-js#readme",
  "bugs": {
    "url": "https://github.com/cuihairu/croupier-sdk-js/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ examples/ --ext .ts,.js",
    "lint:fix": "eslint src/ examples/ --ext .ts,.js --fix",
    "format": "prettier --write 'src/**/*.ts' 'examples/**/*.js'",
    "dev": "ts-node-dev --respawn examples/main.ts",
    "start": "node dist/examples/main.js"
  },
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "files": [
    "dist/",
    "src/",
    "examples/",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}